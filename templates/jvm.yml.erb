- module: http
  metricsets:
    - json
  period: '<%= @check_interval %>'
  hosts: ["http://<%= @fqdn %>:<%= @port %>/management/weblogic/latest/serverRuntime/JVMRuntime?fields=<%= @jvm_fields %>&links=none"]
  namespace: "jvm"
  method: "GET"
  username: "<%= @user %>"
  password: "<%= @pwd %>"
  service:
    name: <%= @service_name %>
  fields:
    hostname: <%= @host %>
    id: weblogic-<%= @service_name %>-<%= @host %>
